{
"name": "GizoogleEverything",
"manifest_version": 2,
"version": "420",
"author": "Aaron Becker",
"description": "See the internet as Snoop Dogg would.",
"icons": {
    "128": "images/icon.png"
    },
"web_accessible_resources": [],
"content_scripts": [{
        "js": ["inject.js"],
        "matches": ["https://*/*","http://*/*"]
    }],
"permissions": [
    "https://*/*",
    "http://*/*",
    "tabs"
  ],
"offline_enabled": true
}