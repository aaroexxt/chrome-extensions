{
"name": "YunReloader",
"manifest_version": 2,
"version": "0",
"author": "Aaron Becker",
"description": "Reloads the page every 10 seconds to prevent the 'White Screen of Death'",
"icons": {
    "128": "images/icon.png"
    },
"web_accessible_resources": [],
"content_scripts": [{
        "js": ["inject.js"],
        "matches": ["https://*/*","http://*/*"]
    }],
"permissions": [
    "https://*/*",
    "http://*/*",
    "tabs"
  ],
"offline_enabled": true
}