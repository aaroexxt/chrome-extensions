{
"name": "CoolCursor",
"manifest_version": 2,
"version": "3.2.9.5",
"author": "Aaron Becker",
"description": "Adds an awesome animated cursor to all web pages. Press Alt-C to show or hide.",
"icons": {
	"128": "/images/icon.png"
	},
"web_accessible_resources": ["cursor.js","/images/cursor.gif"],
"content_scripts": [{
        "js": ["content.js"],
        "matches": ["https://*/*","http://*/*"]
    }],
"permissions": [
    "https://*/*",
    "http://*/*"
  ],
"offline_enabled": true,
"short_name": "CoolCursor"
}